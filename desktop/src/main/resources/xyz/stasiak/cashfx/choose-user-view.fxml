<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<VBox xmlns:fx="http://javafx.com/fxml/1" prefHeight="400.0" prefWidth="600.0" spacing="10.0"
      xmlns="http://javafx.com/javafx/17" fx:controller="xyz.stasiak.cashfx.ChooseUserController">
    <HBox alignment="CENTER_RIGHT" prefHeight="70.0" prefWidth="580.0" spacing="10.0">
        <Button mnemonicParsing="false" onAction="#onCreateButtonAction" text="Create"/>
        <Button mnemonicParsing="false" onAction="#onModifyButtonAction" text="Modify"/>
        <Button mnemonicParsing="false" onAction="#onDeleteButtonAction" text="Delete">
            <HBox.margin>
                <Insets/>
            </HBox.margin>
        </Button>
    </HBox>
    <ListView fx:id="userList" prefHeight="280.0" prefWidth="400.0"/>
    <HBox alignment="CENTER" prefHeight="100.0" prefWidth="580.0" spacing="10.0">
        <Button mnemonicParsing="false" onAction="#onLoginButtonAction" text="Login"/>
    </HBox>
    <opaqueInsets>
        <Insets/>
    </opaqueInsets>
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
    </padding>
</VBox>

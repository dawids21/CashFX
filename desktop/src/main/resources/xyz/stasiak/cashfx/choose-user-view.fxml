<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<VBox xmlns:fx="http://javafx.com/fxml/1" prefHeight="240.0" prefWidth="320.0" spacing="10.0"
      xmlns="http://javafx.com/javafx/17" fx:controller="xyz.stasiak.cashfx.ChooseUserController">
    <ListView fx:id="userList" prefHeight="280.0" prefWidth="400.0"/>
    <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="10.0">
        <Button mnemonicParsing="false" onAction="#onLoginButtonAction" text="Login"/>
        <Button mnemonicParsing="false" onAction="#onCreateUserButtonAction" text="Create user"/>
    </HBox>
    <opaqueInsets>
        <Insets/>
    </opaqueInsets>
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
    </padding>
</VBox>

<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<GridPane xmlns:fx="http://javafx.com/fxml/1" alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
          minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17"
          fx:controller="xyz.stasiak.cashfx.ChooseAccountController">
    <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
    </columnConstraints>
    <rowConstraints>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
    </rowConstraints>
    <ButtonBar prefHeight="40.0" prefWidth="200.0" GridPane.rowIndex="1">
        <buttons>
            <Button mnemonicParsing="false" text="Button"/>
        </buttons>
    </ButtonBar>
    <TableView fx:id="accountsTable" prefHeight="200.0" prefWidth="200.0">
        <columns>
            <TableColumn fx:id="nameTableColumn" prefWidth="75.0" text="Name"/>
            <TableColumn fx:id="moneyTableColumn" prefWidth="75.0" text="Money"/>
            <TableColumn fx:id="typeTableColumn" prefWidth="75.0" text="Type"/>
        </columns>
    </TableView>
</GridPane>
